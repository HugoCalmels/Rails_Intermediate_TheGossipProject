<center>
<h1>EDIT GOSSIP</h1>

<%= form_for :gossip, url: gossip_path, :method => :PUT do |form| %>
  <label for="title">Gossip title</label>
  <%= form.text_field :title %>
  <br />
  <label for="content">Gossip content</label>
  <%= form.text_area :content, size: "60x10" %>
  <%= form.submit %>
<% end %>
</center>